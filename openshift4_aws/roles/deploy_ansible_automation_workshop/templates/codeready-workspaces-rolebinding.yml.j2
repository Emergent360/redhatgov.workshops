  namespace: codeready-workspaces
subjects:
  - kind: User
    apiGroup: rbac.authorization.k8s.io
    name: user{{ item  }}
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: ClusterRole
  name: view
